@model TrashCollectorProject.Models.Service

@{
    ViewData["Title"] = "SetupOTP";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>SetupOTP</h1>

<h4>Service</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SetupOTP">

            <div class="form-group">
                <label asp-for="OneTimePickup" class="control-label"></label>
                <input asp-for="OneTimePickup" class="form-control" type="date"/>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to Dashboard</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
